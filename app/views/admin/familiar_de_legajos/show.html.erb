 
<%=
  present :elegant do |p|
    p.header do |r|
      r.field 'Tipo familiar', @resource.tipo_familiar
    end
    p.body do |r|
      r.field 'Nombre', @resource.nombre
      r.field 'Tipo documento', @resource.tipo_documento
      r.field 'Documento', @resource.documento
      r.field 'C u i f', @resource.c_u_i_f
      r.field 'Nacimiento', @resource.nacimiento
      r.field 'Observaciones', @resource.observaciones
      r.field 'Created at', @resource.created_at
      r.field 'Updated at', @resource.updated_at
    end
    p.actions do |r|
      r.text edit_action
      r.text destroy_action
      r.text back_action
    end
  end
%>
